    <script>
        // Initialize the application
        document.addEventListener('DOMContentLoaded', function() {
            initApp();
        });

        function initApp() {
            // Initialize shopping cart
            initShoppingCart();
            
            // Initialize navigation
            initNavigation();
            
            // Initialize authentication
            initAuthentication();
            
            // Initialize payment system
            initPaymentSystem();
            
            // Initialize chatbot
            initChatbot();
            
            // Initialize resume form
            initResumeForm();
            
            // Initialize self-healing system
            initSelfHealingSystem();
            
            // Initialize event listeners
            initEventListeners();
            
            // Initialize smooth scrolling
            initSmoothScrolling();
        }

        // Shopping Cart Functionality
        function initShoppingCart() {
            let cart = JSON.parse(localStorage.getItem('aiCareerCart')) || [];
            updateCartDisplay();
            
            // Add to cart button
            document.getElementById('addToCart').addEventListener('click', function() {
                const product = {
                    id: 1,
                    name: 'AI Career Accelerator',
                    price: 500,
                    quantity: 1
                };
                
                // Check if product already in cart
                const existingItemIndex = cart.findIndex(item => item.id === product.id);
                
                if (existingItemIndex !== -1) {
                    cart[existingItemIndex].quantity += 1;
                } else {
                    cart.push(product);
                }
                
                localStorage.setItem('aiCareerCart', JSON.stringify(cart));
                updateCartDisplay();
                showNotification('Product added to cart!', 'success');
            });
            
            // Checkout button
            document.getElementById('checkoutBtn').addEventListener('click', function() {
                if (cart.length === 0) {
                    showNotification('Your cart is empty!', 'error');
                    return;
                }
                
                // Check if user is logged in
                const user = JSON.parse(localStorage.getItem('aiCareerUser'));
                if (!user) {
                    showNotification('Please login or register to continue', 'error');
                    openAuthModal();
                    closeCartModal();
                    return;
                }
                
                // Proceed to payment
                openPaymentModal();
                closeCartModal();
            });
            
            // Continue shopping button
            document.getElementById('continueShopping').addEventListener('click', function() {
                closeCartModal();
            });
        }

        function updateCartDisplay() {
            const cart = JSON.parse(localStorage.getItem('aiCareerCart')) || [];
            const cartCount = document.querySelectorAll('.cart-count');
            const cartItems = document.getElementById('cartItems');
            const cartTotal = document.getElementById('cartTotal');
            
            // Update cart count
            const totalItems = cart.reduce((sum, item) => sum + item.quantity, 0);
            cartCount.forEach(count => {
                count.textContent = totalItems;
            });
            
            // Update cart items
            cartItems.innerHTML = '';
            let total = 0;
            
            if (cart.length === 0) {
                cartItems.innerHTML = '<p style="text-align: center; color: var(--gray);">Your cart is empty</p>';
            } else {
                cart.forEach(item => {
                    const itemTotal = item.price * item.quantity;
                    total += itemTotal;
                    
                    const cartItem = document.createElement('div');
                    cartItem.className = 'cart-item';
                    cartItem.innerHTML = `
                        <div class="item-details">
                            <h4>${item.name}</h4>
                            <div class="item-price">R${item.price} x ${item.quantity}</div>
                        </div>
                        <button class="remove-item" data-id="${item.id}">
                            <i class="fas fa-trash"></i>
                        </button>
                    `;
                    
                    cartItems.appendChild(cartItem);
                });
                
                // Add event listeners to remove buttons
                document.querySelectorAll('.remove-item').forEach(button => {
                    button.addEventListener('click', function() {
                        const itemId = parseInt(this.getAttribute('data-id'));
                        removeFromCart(itemId);
                    });
                });
            }
            
            // Update total
            cartTotal.textContent = `R${total}.00`;
        }

        function removeFromCart(itemId) {
            let cart = JSON.parse(localStorage.getItem('aiCareerCart')) || [];
            cart = cart.filter(item => item.id !== itemId);
            localStorage.setItem('aiCareerCart', JSON.stringify(cart));
            updateCartDisplay();
            showNotification('Item removed from cart', 'info');
        }

        function openCartModal() {
            document.getElementById('cartModal').classList.add('active');
            document.getElementById('overlay').classList.add('active');
            document.body.style.overflow = 'hidden';
        }

        function closeCartModal() {
            document.getElementById('cartModal').classList.remove('active');
            document.getElementById('overlay').classList.remove('active');
            document.body.style.overflow = 'auto';
        }

        // Navigation Functionality
        function initNavigation() {
            const hamburger = document.getElementById('hamburger');
            const mobileNav = document.getElementById('mobileNav');
            const closeMenu = document.getElementById('closeMenu');
            const overlay = document.getElementById('overlay');
            const cartIcon = document.getElementById('cartIcon');
            const mobileCartIcon = document.getElementById('mobileCartIcon');
            const closeCart = document.getElementById('closeCart');
            
            // Hamburger menu toggle
            hamburger.addEventListener('click', function() {
                mobileNav.classList.add('active');
                overlay.classList.add('active');
                document.body.style.overflow = 'hidden';
            });
            
            // Close mobile menu
            closeMenu.addEventListener('click', function() {
                mobileNav.classList.remove('active');
                overlay.classList.remove('active');
                document.body.style.overflow = 'auto';
            });
            
            // Cart icon click
            cartIcon.addEventListener('click', openCartModal);
            mobileCartIcon.addEventListener('click', function() {
                mobileNav.classList.remove('active');
                openCartModal();
            });
            
            // Close cart
            closeCart.addEventListener('click', closeCartModal);
            
            // Close modals when clicking overlay
            overlay.addEventListener('click', function() {
                mobileNav.classList.remove('active');
                closeCartModal();
                closeAuthModal();
                closePaymentModal();
            });
        }

        // Authentication Functionality
        function initAuthentication() {
            const showLoginBtn = document.getElementById('showLogin');
            const showRegisterBtn = document.getElementById('showRegister');
            const mobileLoginBtn = document.getElementById('mobileLogin');
            const closeAuthBtn = document.getElementById('closeAuth');
            const authTabs = document.querySelectorAll('.auth-tab');
            const loginForm = document.getElementById('loginForm');
            const registerForm = document.getElementById('registerForm');
            const loginSubmit = document.getElementById('loginForm');
            const registerSubmit = document.getElementById('registerForm');
            
            // Show auth modal
            [showLoginBtn, showRegisterBtn, mobileLoginBtn].forEach(btn => {
                if (btn) {
                    btn.addEventListener('click', function(e) {
                        e.preventDefault();
                        openAuthModal();
                        
                        // Switch to appropriate tab
                        if (this.id === 'showRegister' || this.id === 'mobileLogin') {
                            switchAuthTab('register');
                        } else {
                            switchAuthTab('login');
                        }
                    });
                }
            });
            
            // Close auth modal
            closeAuthBtn.addEventListener('click', closeAuthModal);
            
            // Switch between login and register tabs
            authTabs.forEach(tab => {
                tab.addEventListener('click', function() {
                    const tabName = this.getAttribute('data-tab');
                    switchAuthTab(tabName);
                });
            });
            
            // Login form submission
            loginSubmit.addEventListener('submit', function(e) {
                e.preventDefault();
                handleLogin();
            });
            
            // Register form submission
            registerSubmit.addEventListener('submit', function(e) {
                e.preventDefault();
                handleRegistration();
            });
        }

        function openAuthModal() {
            document.getElementById('authModal').classList.add('active');
            document.getElementById('overlay').classList.add('active');
            document.body.style.overflow = 'hidden';
        }

        function closeAuthModal() {
            document.getElementById('authModal').classList.remove('active');
            document.getElementById('overlay').classList.remove('active');
            document.body.style.overflow = 'auto';
        }

        function switchAuthTab(tabName) {
            // Update active tab
            document.querySelectorAll('.auth-tab').forEach(tab => {
                tab.classList.remove('active');
                if (tab.getAttribute('data-tab') === tabName) {
                    tab.classList.add('active');
                }
            });
            
            // Show active form
            document.querySelectorAll('.auth-form').forEach(form => {
                form.classList.remove('active');
            });
            document.getElementById(tabName + 'Form').classList.add('active');
        }

        function handleLogin() {
            const email = document.getElementById('loginEmail').value;
            const password = document.getElementById('loginPassword').value;
            
            // Simple validation
            if (!email || !password) {
                showNotification('Please fill in all fields', 'error');
                return;
            }
            
            // Simulate API call
            showNotification('Logging in...', 'info');
            
            setTimeout(() => {
                // Check if user exists
                const users = JSON.parse(localStorage.getItem('aiCareerUsers')) || [];
                const user = users.find(u => u.email === email && u.password === password);
                
                if (user) {
                    // Store user session
                    localStorage.setItem('aiCareerUser', JSON.stringify(user));
                    showNotification('Login successful!', 'success');
                    closeAuthModal();
                    
                    // Update UI for logged in user
                    updateUserUI(user);
                } else {
                    showNotification('Invalid email or password', 'error');
                }
            }, 1500);
        }

        function handleRegistration() {
            const fullName = document.getElementById('fullName').value;
            const idNumber = document.getElementById('idNumber').value;
            const email = document.getElementById('email').value;
            const phone = document.getElementById('phone').value;
            const password = document.getElementById('password').value;
            const confirmPassword = document.getElementById('confirmPassword').value;
            const terms = document.getElementById('terms').checked;
            
            // Validation
            if (!fullName || !idNumber || !email || !phone || !password || !confirmPassword) {
                showNotification('Please fill in all fields', 'error');
                return;
            }
            
            if (password !== confirmPassword) {
                showNotification('Passwords do not match', 'error');
                return;
            }
            
            if (!terms) {
                showNotification('Please accept the terms and conditions', 'error');
                return;
            }
            
            // Validate South African ID (simple check for 13 digits)
            if (!/^\d{13}$/.test(idNumber)) {
                showNotification('Please enter a valid South African ID number (13 digits)', 'error');
                return;
            }
            
            // Validate phone number (simple South African format)
            if (!/^(\+27|0)[6-8][0-9]{8}$/.test(phone.replace(/\s/g, ''))) {
                showNotification('Please enter a valid South African phone number', 'error');
                return;
            }
            
            // Check if user already exists
            const users = JSON.parse(localStorage.getItem('aiCareerUsers')) || [];
            if (users.find(u => u.email === email)) {
                showNotification('An account with this email already exists', 'error');
                return;
            }
            
            // Create new user
            const newUser = {
                id: Date.now(),
                fullName,
                idNumber,
                email,
                phone,
                password, // In a real app, this would be hashed
                registrationDate: new Date().toISOString(),
                isActive: true
            };
            
            users.push(newUser);
            localStorage.setItem('aiCareerUsers', JSON.stringify(users));
            localStorage.setItem('aiCareerUser', JSON.stringify(newUser));
            
            showNotification('Registration successful!', 'success');
            closeAuthModal();
            
            // Update UI for logged in user
            updateUserUI(newUser);
        }

        function updateUserUI(user) {
            // Update auth buttons to show user is logged in
            const authButtons = document.querySelector('.auth-buttons');
            if (authButtons) {
                authButtons.innerHTML = `
                    <span style="color: var(--gold);">Welcome, ${user.fullName.split(' ')[0]}</span>
                    <button class="btn btn-outline" id="logoutBtn">Logout</button>
                `;
                
                document.getElementById('logoutBtn').addEventListener('click', handleLogout);
            }
            
            // Update mobile nav
            const mobileLogin = document.getElementById('mobileLogin');
            if (mobileLogin) {
                mobileLogin.innerHTML = `Welcome, ${user.fullName.split(' ')[0]}`;
                mobileLogin.style.color = 'var(--gold)';
                mobileLogin.style.cursor = 'default';
                
                // Add logout to mobile nav
                const logoutLi = document.createElement('li');
                logoutLi.innerHTML = `<a href="#" id="mobileLogout">Logout</a>`;
                document.querySelector('.mobile-nav ul').appendChild(logoutLi);
                
                document.getElementById('mobileLogout').addEventListener('click', function(e) {
                    e.preventDefault();
                    handleLogout();
                });
            }
        }

        function handleLogout() {
            localStorage.removeItem('aiCareerUser');
            showNotification('Logged out successfully', 'info');
            
            // Reload page to reset UI
            setTimeout(() => {
                window.location.reload();
            }, 1000);
        }

        // Payment System
        function initPaymentSystem() {
            const closePaymentBtn = document.getElementById('closePayment');
            const paymentMethods = document.querySelectorAll('.payment-method');
            const processPaymentBtn = document.getElementById('processPayment');
            
            // Close payment modal
            closePaymentBtn.addEventListener('click', closePaymentModal);
            
            // Select payment method
            paymentMethods.forEach(method => {
                method.addEventListener('click', function() {
                    paymentMethods.forEach(m => m.classList.remove('active'));
                    this.classList.add('active');
                });
            });
            
            // Process payment
            processPaymentBtn.addEventListener('click', processPayment);
        }

        function openPaymentModal() {
            document.getElementById('paymentModal').classList.add('active');
            document.getElementById('overlay').classList.add('active');
            document.body.style.overflow = 'hidden';
        }

        function closePaymentModal() {
            document.getElementById('paymentModal').classList.remove('active');
            document.getElementById('overlay').classList.remove('active');
            document.body.style.overflow = 'auto';
        }

        function processPayment() {
            const selectedMethod = document.querySelector('.payment-method.active');
            
            if (!selectedMethod) {
                showNotification('Please select a payment method', 'error');
                return;
            }
            
            const paymentMethod = selectedMethod.dataset.method;
            const processBtn = document.getElementById('processPayment');
            const originalText = processBtn.textContent;
            
            // Show loading state
            processBtn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Processing Payment...';
            processBtn.disabled = true;
            
            // Simulate payment processing
            setTimeout(() => {
                // Record payment in localStorage
                const payment = {
                    id: 'pay_' + Date.now(),
                    userId: JSON.parse(localStorage.getItem('aiCareerUser')).id,
                    amount: 500,
                    method: paymentMethod,
                    status: 'completed',
                    date: new Date().toISOString()
                };
                
                const payments = JSON.parse(localStorage.getItem('aiCareerPayments')) || [];
                payments.push(payment);
                localStorage.setItem('aiCareerPayments', JSON.stringify(payments));
                
                // Clear cart
                localStorage.setItem('aiCareerCart', JSON.stringify([]));
                updateCartDisplay();
                
                showNotification('Payment successful! You now have full access to our AI job application service.', 'success');
                closePaymentModal();
                
                // Reset button
                processBtn.textContent = originalText;
                processBtn.disabled = false;
                
                // Simulate weekly payout distribution
                simulateWeeklyPayout(500);
            }, 2000);
        }

        function simulateWeeklyPayout(amount) {
            // Simulate the payout distribution as requested
            const payout = {
                date: new Date().toISOString(),
                total: amount,
                distributions: {
                    ownerFNB: amount * 0.4, // 40%
                    ownerAfricanBank: amount * 0.15, // 15%
                    reserveFNB: amount * 0.2, // 20%
                    aiFNB: amount * 0.2, // 20%
                    remaining: amount * 0.05 // 5% remains
                }
            };
            
            const payouts = JSON.parse(localStorage.getItem('aiCareerPayouts')) || [];
            payouts.push(payout);
            localStorage.setItem('aiCareerPayouts', JSON.stringify(payouts));
            
            console.log('Weekly payout simulated:', payout);
        }

        // Chatbot Functionality
        function initChatbot() {
            const chatbotToggle = document.getElementById('chatbotToggle');
            const chatbotWindow = document.getElementById('chatbotWindow');
            const sendMessage = document.getElementById('sendMessage');
            const chatInput = document.getElementById('chatInput');
            const chatbotMessages = document.getElementById('chatbotMessages');
            
            // Toggle chatbot window
            chatbotToggle.addEventListener('click', function() {
                chatbotWindow.classList.toggle('active');
            });
            
            // Send message
            sendMessage.addEventListener('click', function() {
                sendChatMessage();
            });
            
            // Send message on Enter key
            chatInput.addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    sendChatMessage();
                }
            });
        }

        function sendChatMessage() {
            const chatInput = document.getElementById('chatInput');
            const chatbotMessages = document.getElementById('chatbotMessages');
            const message = chatInput.value.trim();
            
            if (message === '') return;
            
            // Add user message
            const userMessage = document.createElement('div');
            userMessage.className = 'message user-message';
            userMessage.textContent = message;
            chatbotMessages.appendChild(userMessage);
            
            // Clear input
            chatInput.value = '';
            
            // Scroll to bottom
            chatbotMessages.scrollTop = chatbotMessages.scrollHeight;
            
            // Simulate AI response
            setTimeout(() => {
                const botResponse = generateBotResponse(message);
                const botMessage = document.createElement('div');
                botMessage.className = 'message bot-message';
                botMessage.textContent = botResponse;
                chatbotMessages.appendChild(botMessage);
                
                // Scroll to bottom
                chatbotMessages.scrollTop = chatbotMessages.scrollHeight;
            }, 1000);
        }

        function generateBotResponse(message) {
            const lowerMessage = message.toLowerCase();
            
            if (lowerMessage.includes('hello') || lowerMessage.includes('hi') || lowerMessage.includes('hey')) {
                return "Hello! I'm Bronwyn, your AI career assistant. How can I help you with your job search today?";
            } else if (lowerMessage.includes('price') || lowerMessage.includes('cost') || lowerMessage.includes('fee')) {
                return "Our AI Career Accelerator service costs R500 as a one-time payment for lifetime access. There are no hidden fees or additional charges!";
            } else if (lowerMessage.includes('how it works') || lowerMessage.includes('process') || lowerMessage.includes('work')) {
                return "It's simple: 1) Register and pay R500, 2) Upload your documents, 3) Our AI applies to jobs for you, 4) You get hired! The AI stops once you're employed.";
            } else if (lowerMessage.includes('cv') || lowerMessage.includes('resume')) {
                return "Our AI can both enhance your existing CV or create a new one from scratch if you don't have one. We'll make sure it's optimized for your target industry.";
            } else if (lowerMessage.includes('contact') || lowerMessage.includes('support') || lowerMessage.includes('help')) {
                return "You can reach our support team at support@costbyte.co.za or call/WhatsApp us at 072 142 3125. We're here to help!";
            } else if (lowerMessage.includes('payment') || lowerMessage.includes('pay') || lowerMessage.includes('checkout')) {
                return "We accept PayFast, PayShap, and direct EFT payments. All transactions are secure and encrypted.";
            } else if (lowerMessage.includes('south africa') || lowerMessage.includes('sa')) {
                return "Yes, our service is specifically designed for South African job seekers. We comply with all local regulations and focus on the South African job market.";
            } else {
                return "I understand you're asking about '" + message + "'. For detailed information about our services, pricing, or how to get started, please visit our website or contact our support team at support@costbyte.co.za.";
            }
        }

        // Resume Form Functionality
        function initResumeForm() {
            const resumeForm = document.getElementById('resumeForm');
            const clearFormBtn = document.getElementById('clearResumeForm');
            
            // Submit resume form
            resumeForm.addEventListener('submit', function(e) {
                e.preventDefault();
                generateResume();
            });
            
            // Clear form
            clearFormBtn.addEventListener('click', function() {
                document.getElementById('resumeForm').reset();
                showNotification('Form cleared', 'info');
            });
        }

        function generateResume() {
            const fullName = document.getElementById('resumeFullName').value;
            const email = document.getElementById('resumeEmail').value;
            const phone = document.getElementById('resumePhone').value;
            const location = document.getElementById('resumeLocation').value;
            const profession = document.getElementById('resumeProfession').value;
            
            // Basic validation
            if (!fullName || !email || !phone || !location || !profession) {
                showNotification('Please fill in all required fields', 'error');
                return;
            }
            
            // Show loading state
            const submitBtn = document.querySelector('#resumeForm button[type="submit"]');
            const originalText = submitBtn.textContent;
            submitBtn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Generating...';
            submitBtn.disabled = true;
            
            // Simulate AI resume generation
            setTimeout(() => {
                // Store resume data
                const resumeData = {
                    id: 'resume_' + Date.now(),
                    fullName,
                    email,
                    phone,
                    location,
                    profession,
                    summary: document.getElementById('resumeSummary').value,
                    experience: document.getElementById('resumeExperience').value,
                    education: document.getElementById('resumeEducation').value,
                    skills: document.getElementById('resumeSkills').value,
                    generatedAt: new Date().toISOString()
                };
                
                const resumes = JSON.parse(localStorage.getItem('aiCareerResumes')) || [];
                resumes.push(resumeData);
                localStorage.setItem('aiCareerResumes', JSON.stringify(resumes));
                
                // Reset button
                submitBtn.textContent = originalText;
                submitBtn.disabled = false;
                
                // Show success message with download option
                showNotification('Resume generated successfully! <a href="#" style="color: white; text-decoration: underline;" onclick="downloadResume()">Download PDF</a>', 'success');
                
                // Clear form
                document.getElementById('resumeForm').reset();
            }, 2000);
        }

